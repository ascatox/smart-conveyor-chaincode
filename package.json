        "@types/chai": "^4.0.4",
        "@types/mocha": "^2.2.43",
        "@types/node": "^9.4.6",
        "@types/winston": "^2.3.8",
        "chai": "^4.1.2",
        "coveralls": "^3.0.0",
        "husky": "^0.15.0-rc.8",
